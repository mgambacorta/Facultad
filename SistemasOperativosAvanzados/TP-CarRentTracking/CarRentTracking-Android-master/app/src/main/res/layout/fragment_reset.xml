<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.carrent.carrent.ResetFragment">

    <!-- TODO: Update blank fragment layout -->

    <Button
        android:id="@+id/btnClearAll"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:text="Eliminar TRACKING Y EVENTOS"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/btnClearEvents"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="65dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="65dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnClearTracking"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="ELIMINAR TRACKING"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="123dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnClearEvents"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="ELIMINAR EVENTOS"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnClearTracking"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btnClearTracking"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnClearTracking" />
</android.support.constraint.ConstraintLayout>
